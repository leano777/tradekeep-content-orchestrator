# Database
DATABASE_URL="file:./dev.db"

# JWT Secret
JWT_SECRET=your-secret-key-here

# Server Port
PORT=9000

# Google Drive OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:9000/api/v1/assets/cloud/callback/google

# Microsoft OneDrive OAuth
MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret
MICROSOFT_REDIRECT_URI=http://localhost:9000/api/v1/assets/cloud/callback/microsoft

# Email Service (Resend)
RESEND_API_KEY=your-resend-api-key
EMAIL_FROM=noreply@yourdomain.com

# Features
ENABLE_COLLABORATION=true
ENABLE_AI_GENERATION=false
ENABLE_EMAIL_CAMPAIGNS=true
ENABLE_SOCIAL_PUBLISHING=true
ENABLE_ANALYTICS=true